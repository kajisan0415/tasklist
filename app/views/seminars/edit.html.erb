<%= form_with model: @seminar, local: true do |f| %>

    <div class="article-form-group">
        <div class="article-form-left">
            <p class"article-form-label">タイトル <span class="article-form-must">必須</span></p>
        </div>
        <div class="article-form-right">
            <p><%= f.text_field :title, class: "article_image" %></p>
        </div>
    </div>

    <div class="article-form-group">
        <div class="article-form-left">
            <p class"article-form-label">開始 <span class="article-form-must">必須</span></p>
        </div>
        
        <div class="article-form-right">
            <p><%= f.datetime_select :starts_at %></p>
        </div>
    </div>
    
    <div class="article-form-group">
        <div class="article-form-left">
            <p class"article-form-label">終わり <span class="article-form-must">必須</span></p>
        </div>
        
        <div class="article-form-right">
            <p><%= f.datetime_select :ends_at %></p>
        </div>
    </div>
    
    <tr class="actions">
      <%= f.submit "更新" ,class: 'btn btn-sm btn-success' %>
    </tr>
  <% end %>